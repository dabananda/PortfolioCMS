{
  "IdentitySettings": {
    "RequireConfirmedEmail": true,
    "PasswordRequireDigit": true,
    "PasswordRequireLowercase": true,
    "PasswordRequireUppercase": true,
    "PasswordRequireNonAlphanumeric": true,
    "PasswordRequiredLength": 8,
    "MaxFailedAccessAttempts": 5,
    "LockoutTimeSpanMinutes": 15
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "CorsSettings": {
    "AllowedOrigins": [
      "http://localhost:5173",
      "https://pcmsdm.azurewebsites.net"
    ]
  },
  "FrontendBaseUrl": "https://pcmsdm.azurewebsites.net"
}